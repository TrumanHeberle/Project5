package prj5;

import java.util.Comparator;

/**
 * @author AshlynUser
 *
 */
public class CompareSongYear implements Comparator<Object> {
    
    /**
     * compares two songs based on the year
     */
    @Override
    public int compare(Object o1, Object o2)
    {
        int diff = ((Song)o1).getYear() - ((Song)o2).getYear();
        return diff;
    }

}
